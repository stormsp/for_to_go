func ira10(bp,cod)
  dout[bp+0]=ne(cod & 1, 0) 	;звено в полож открыто-закрыто 1-2!!!
  dout[bp+1]=ne(cod & 4, 0) 	;Df10
  dout[bp+2]=ne(cod & 8, 0) 	;Df20
  dout[bp+3]=ne(cod & 32, 0) 	;разряд элемента питания
  dout[bp+4]=ne(cod & 128, 0) 	;Режим управления МУ/ДУ
  cod=int(cod/256)
  dout[bp+5]=ne(cod & 1, 0)	;открытие закрытие стоп 1-2-4!!!!
  dout[bp+6]=ne(cod & 8, 0)	;Df24
  dout[bp+7]=ne(cod & 32, 0)	;Включен нагрев
  dout[bp+8]=ne(cod & 128, 0)	;Готов к техн операц
  return(0)                                       
endfunc
