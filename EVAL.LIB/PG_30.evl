
func pg30(bp,cod1,cod2,cod3)
  dout[bp+0]=ne(cod1 & 2, 0) 	;Клапан заправки
  dout[bp+1]=ne(cod1 & 4, 0) 	;Клапан пульсатора
  dout[bp+2]=ne(cod1 & 16, 0) 	;Клапан сброса
  dout[bp+3]=ne(cod1 & 32, 0) 	;Проникновение в одоризатор
  cod=int(cod1/256)
  dout[bp+4]=ne(cod1 & 2, 0)+2*ne(cod1 & 4, 0) 	;Низкая/Высокая температура
  dout[bp+5]=ne(cod1 & 8, 0)	;Высокое давление в коллекторе
  dout[bp+6]=ne(cod1 & 16, 0)	;Высокий перепад давления 
  dout[bp+7]=ne(cod1 & 32, 0)+2*ne(cod1 & 64, 0);Низкий/Высокий уровень в расходной емкости
  dout[bp+8]=ne(cod1 & 128, 0)	;Ошибка выдачи дозы

  dout[bp+9]=ne(cod2 & 1, 0)	;Авария
  dout[bp+10]=ne(cod2 & 2, 0)	;Пожар
  dout[bp+11]=ne(cod2 & 4, 0) 	;Обрыв датчика потока
  dout[bp+12]=ne(cod2 & 8, 0)	;Неисправность датчика давления в коллекторе
  dout[bp+13]=ne(cod2 & 16, 0)	;Неисправность датчика давления в емкости
  dout[bp+14]=ne(cod2 & 32, 0) 	;Неисправность сигнализатора уровня
  dout[bp+15]=ne(cod2 & 64, 0)	;Неисправность датчика температуры
  cod=int(cod2/256)
  dout[bp+16]=ne(cod2 & 1, 0)	;РИП норма
  dout[bp+17]=ne(cod2 & 2, 0)	;РИП батарея норма
  dout[bp+18]=ne(cod2 & 8, 0)	;Пульсатор
endfunc




